{
	"name": "Append to Title",
	"version": "2.0",
	"description": "Append specific, configurable strings to the title of the tab based on the URL.",
	"manifest_version": 3,
	"permissions": [
		"tabs",
		"storage",
		"scripting"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"icons": {
		"128": "icon128.png"
	},
	"background": {
		"service_worker": "service_worker.js"
	},
	"incognito": "split",
	"options_page": "options.html"
}